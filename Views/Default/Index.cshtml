@model WebApplication1.Models.Classes.BlogIE
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}


<!DOCTYPE HTML>
<html>
<body>


    <div class="header-banner">
        <div class="wrap">
            <div class="banner">
                <div class="indent">
                    <h2 class="indent-left margin-bot">Shelters in Toronto</h2>
                    <div class="banner-left">
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/helping-hand.png" height="68px" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Help</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/donation.png" height="68px" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Donate</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/agreement.png" height="68px" />
                            </div>
                            <div class="grid1-desc">
                                <p>A Home</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/contact.png" height="68px" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Reach Us</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="header_bottom_right_images">

                        <div class="slider" style="width:650px; height:350px;">
                            <ul class="rslides" id="slider1">
                                
                                @foreach (var x in Model.Value1)
                                {
                                    <li><div style="width:650px; height:350px; background:url(@x.image);"></div></li>
                                }

                            </ul>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class"content">
        <div class="wrap">
            <div class="wrapper">
                <div class="con-top">
                    <h3>Welcome to A Warm Home!</h3>
                    <p>Do you need a shelter? Had to live in streets? No more! Reach us and we will get you a home.</p>
                </div>
                @Html.Action("Partial1","Default")
                
            </div>
        </div>
    </div>

</body>
</html>


